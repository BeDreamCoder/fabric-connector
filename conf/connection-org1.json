        "BCCSP": {
            "security": {
                "enabled": true,
                "default": {
                    "provider": "SW"
                },
                "hashAlgorithm": "SHA2",
                "softVerify": true,
                "level": 256
            }
        },
        "cryptoconfig": {
            "path": "/tmp/fabric/organizations"
        }
    },
    "channels": {
        "mychannel": {
            "peers": {
                "peer0.org1.example.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
